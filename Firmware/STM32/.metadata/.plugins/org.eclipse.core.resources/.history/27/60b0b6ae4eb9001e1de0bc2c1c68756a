/* Defines*/
#define ADP5360_I2C_ADDR 	(0x57 << 1) /* p. 5 */
#define ADP5360_PART_ID		0x1
#define ADP5360_REVISION_ID	0x0

#define MAX30102_REG_INT1
#define MAX30102_REG_INT2
#define MAX30102_REG_INT3
#define MAX30102_REG_INT4
