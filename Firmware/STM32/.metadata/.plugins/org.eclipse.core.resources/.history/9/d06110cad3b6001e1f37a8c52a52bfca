M24256X_StatusTypeDef M24256X_ReadMemory(M24256X_HandleTypeDef *dev, uint8_t *data, uint16_t address){



	HAL_I2C_Mem_Read(dev->i2cHandle, ADP5360_I2C_ADDR, reg, I2C_MEMADD_SIZE_8BIT, data, 1, HAL_MAX_DELAY);
}

M24256X_StatusTypeDef M24256X_WriteMemory(M24256X_HandleTypeDef *dev, uint8_t *data, uint16_t address){

}

